program.sub.drpmode_click.start

F.Intrinsic.Debug.SetLA("Case structure that enables or disables various fields based on the Mode selected.  Also sets the global variable sMode as either N, O, or D")

F.Intrinsic.Control.SelectCase(V.Screen.F_Mode!drpMode.Text)
	F.Intrinsic.Control.Case("New Template")
		V.Global.sMode.Set("N")
		Gui.F_Template.cmdSave.Enabled(True)
		Gui.F_Template.cmdSave.Visible(True)
		Gui.F_Template.txtDisplay.Enabled(True)
		Gui.F_Template.txtDisplay.Visible(True)
	F.Intrinsic.Control.Case("Edit Template")
		V.Global.sMode.Set("O")
		Gui.F_Template.cmdSave.Enabled(true)
		Gui.F_Template.cmdSave.Visible(true)
		Gui.F_Template.drpDisplay.Enabled(true)
		Gui.F_Template.drpDisplay.Visible(true)
		F.Intrinsic.Control.CallSub(Populate_templates)
	F.Intrinsic.Control.Case("Delete Template")
		V.Global.sMode.Set("D")
		Gui.F_Template.txtFileName.Enabled(false)
		Gui.F_Template.drpDisplay.Enabled(true)
		Gui.F_Template.drpDisplay.Visible(true)
		Gui.F_Template.cmdDelete.enabled(true)
		Gui.F_Template.cmdDelete.Visible(true)
		Gui.F_Template.chkDescr1.Enabled(false)
		Gui.F_Template.chkDescr2.Enabled(false)
		Gui.F_Template.chkDescr3.Enabled(false)
		F.Intrinsic.Control.CallSub(Populate_templates)
F.Intrinsic.Control.EndSelect

F.Intrinsic.Debug.SetLA("Hides the mode selection form and shows the Template Maint form")
Gui.F_Mode..Visible(false)
Gui.F_Template..Show

program.sub.drpmode_click.end

