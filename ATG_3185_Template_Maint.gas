program.sub.cmdsave_click.start
Variable.Local.sDisplay.Declare(String)
V.Local.sQuery.Declare(String)
V.Local.bWrite.Declare(Boolean)
F.Intrinsic.Debug.Stop
F.Intrinsic.Debug.SetLA("Reads current mode and sets sDisplay to corresponding value, either from the txt or the drp")
F.Intrinsic.Control.If(V.Global.sMode,=,"N")
	V.Local.sDisplay.Set(V.Screen.F_Template!txtDisplay.Text)
F.Intrinsic.Control.ElseIf(V.Global.sMode,=,"O")
	V.Local.sDisplay.Set(V.Screen.f_template!drpdisplay.Text)
F.Intrinsic.Control.EndIf

F.Intrinsic.Debug.SetLA("Checks to see if sDisplay is blank, then notifies user and exits sub if true")
F.Intrinsic.Control.If(V.Local.sDisplay,=,"")
	F.Intrinsic.Control.If(V.Global.sMode,=,"N")
		F.Intrinsic.UI.Msgbox("You must provide a name for the new template")
	F.Intrinsic.Control.ElseIf(V.Global.sMode,=,"O")
		F.Intrinsic.UI.Msgbox("You must select a template to edit")
	F.Intrinsic.Control.EndIf
	F.Intrinsic.Control.ExitSub
F.Intrinsic.Control.EndIf

F.Intrinsic.Debug.SetLA("Checks to see if filename is blank, then notifies user to enter filename and exits sub")
F.Intrinsic.Control.If(V.Screen.F_Template!txtFileName.text,=,"")
	F.Intrinsic.UI.Msgbox("Please enter a filename")
	F.Intrinsic.Control.ExitSub
F.Intrinsic.Control.EndIf

F.Intrinsic.Debug.SetLA("Checks filename to see if it is a valid Windows filename")
F.Intrinsic.Control.CallSub(Validfilename)
F.Intrinsic.Control.If(V.Args.bRet,=,False)
	F.Intrinsic.Control.ExitSub
F.Intrinsic.Control.EndIf

F.Intrinsic.Debug.SetLA("Disables save button and form")
Gui.F_Template.cmdSave.enabled(false)
Gui.F_Template..Enabled(False)

V.Local.bWrite.Set(False)

F.Intrinsic.Control.If(V.Global.sMode,=,"N")
	F.Intrinsic.String.Concat("SELECT * FROM ATG_3185_TEMPLATES WHERE DISPLAY = '",V.Screen.F_Template!txtDisplay.Text,"'",V.Local.sQuery)
	F.ODBC.Connection!conx.OpenRecordsetRW("rstTemplate",V.Local.sQuery)
	F.Intrinsic.Control.If(V.ODBC.conx!rstTemplate.EOF,<>,True)
		F.Intrinsic.UI.Msgbox("A template with this display name already exists")
		Gui.F_Template.cmdSave.Enabled(True)
		Gui.F_Template..Enabled(true)
		F.ODBC.conx!rstTemplate.Close
		F.Intrinsic.Control.ExitSub
	F.Intrinsic.Control.EndIf

	F.ODBC.conx!rstTemplate.AddNew
	F.ODBC.conx!rstTemplate.Set!DISPLAY(V.Screen.F_Template!txtDisplay.Text)

	V.Local.bWrite.Set(True)
F.Intrinsic.Control.Else
	F.Intrinsic.string.Concat("SELECT * FROM ATG_3185_TEMPLATES WHERE TEMPLATE_ID = ",V.Screen.F_TEMPLATE!drpDisplay.ItemData,V.Local.sQuery)
	F.ODBC.Connection!conx.OpenRecordsetRW("rstTemplate",V.Local.sQuery)
	F.Intrinsic.Control.If(V.ODBC.conx!rstTemplate.EOF,<>,True)
		V.Local.bWrite.Set(True)
	F.Intrinsic.Control.Else
		F.Intrinsic.UI.Msgbox("This template no longer exists")
	F.Intrinsic.Control.EndIf
F.Intrinsic.Control.EndIf
F.Intrinsic.Control.If(V.Local.bWrite,=,True)
	F.ODBC.conx!rstTemplate.Set!FILE_NAME(V.Screen.F_Template!txtFileName.Text)
	F.ODBC.conx!rstTemplate.Set!DESCR_1(V.Screen.F_Template!chkDescr1.Value)
	F.ODBC.conx!rstTemplate.Set!DESCR_2(V.Screen.F_Template!chkDescr2.Value)
	F.ODBC.conx!rstTemplate.Set!DESCR_3(V.Screen.F_Template!chkDescr3.Value)
	F.ODBC.conx!rstTemplate.Update
F.Intrinsic.Control.endif
F.ODBC.conx!rstTemplate.Close

F.Intrinsic.Control.CallSub(Clear)

Gui.F_Template.cmdSave.Enabled(True)
Gui.F_Template..Enabled(true)




	
program.sub.cmdsave_click.end

program.sub.cmddelete_click.start
V.Local.sDisplay.Declare(String)
V.Local.sDisplay.Set(V.Screen.f_template!drpdisplay.Text)
V.Local.sQuery.Declare(String)
V.Local.iTemplate.declare(Long)

F.Intrinsic.Control.If(V.Screen.F_Template!drpDisplay.Text,<>,"")
		
	Gui.F_Template.cmdSave.enabled(false)
	Gui.F_Template..Enabled(False)
	V.Local.iTemplate.Set(V.Screen.F_Template!drpDisplay.ItemData)
	Gui.F_Template.drpDisplay.RemoveItem(V.Screen.F_Template!drpDisplay.ListIndex)
	F.Intrinsic.string.Concat("DELETE FROM ATG_3185_TEMPLATES WHERE TEMPLATE_ID = ",V.local.iTemplate,V.Local.sQuery)

	F.ODBC.Connection!conX.Execute(V.Local.sQuery)

	F.Intrinsic.Control.CallSub(Clear)

	Gui.F_Template.cmdSave.Enabled(True)
	Gui.F_Template..Enabled(true)

F.Intrinsic.Control.Else
	F.Intrinsic.UI.Msgbox("Please select a template to delete")
F.Intrinsic.Control.EndIf









program.sub.cmddelete_click.end

Program.Sub.Clear.Start
Gui.F_Template.chkDescr1.Value(0)
Gui.F_Template.chkDescr2.Value(0)
Gui.F_Template.chkDescr3.Value(0)
Gui.F_Template.drpDisplay.Text("")
Gui.F_Template.txtDisplay.Text("")
Gui.F_Template.txtFileName.Text("")


Program.Sub.Clear.End

Program.Sub.Populate_Templates.Start
V.Local.sQuery.Declare(String)
V.Local.sQuery.Set("SELECT * FROM ATG_3185_TEMPLATES ORDER BY DISPLAY ASC")

F.ODBC.Connection!conx.OpenRecordsetRO("rstTemplate",V.Local.sQuery)
F.Intrinsic.Control.DoUntil(V.ODBC.conx!rstTemplate.EOF,=,True)
	Gui.F_Template.drpDisplay.AddItem(V.ODBC.conx!rstTemplate.FieldVal!DISPLAY,V.ODBC.conx!rstTemplate.FieldVal!TEMPLATE_ID)
	F.ODBC.conx!rstTemplate.MoveNext
F.Intrinsic.Control.Loop

F.ODBC.conx!rstTemplate.Close

Program.Sub.Populate_Templates.End

program.sub.drpdisplay_click.start
V.Local.sQuery.Declare(String)

F.Intrinsic.Control.If(V.Screen.F_Template!drpDisplay.Text,<>,"")
	Gui.F_Template..enabled(false)

	F.Intrinsic.string.Concat("SELECT * FROM ATG_3185_TEMPLATES WHERE TEMPLATE_ID =",V.Screen.F_Template!drpDisplay.ItemData,V.Local.sQuery)
	F.ODBC.Connection!conx.OpenRecordsetRO("rstTemplate",V.Local.sQuery)
	F.Intrinsic.Control.If(V.ODBC.conx!rstTemplate.EOF,<>,True)
		Gui.F_Template.txtFileName.Text(V.ODBC.conx!rstTemplate.FieldValTrim!FILE_NAME)
		F.Intrinsic.Control.If(V.ODBC.conx!rstTemplate.FieldVal!DESCR_1,=,True)
			Gui.F_Template.chkDescr1.Value(1)
		F.Intrinsic.Control.Else
			Gui.F_Template.chkDescr1.Value(0)
		F.Intrinsic.Control.EndIf
		F.Intrinsic.Control.If(V.ODBC.conx!rstTemplate.FieldVal!DESCR_2,=,True)
			Gui.F_Template.chkDescr2.Value(1)
		F.Intrinsic.Control.Else
			Gui.F_Template.chkDescr2.Value(0)
		F.Intrinsic.Control.EndIf
		F.Intrinsic.Control.If(V.ODBC.conx!rstTemplate.FieldVal!DESCR_3,=,True)
			Gui.F_Template.chkDescr3.Value(1)
		F.Intrinsic.Control.Else
			Gui.F_Template.chkDescr3.Value(0)
		F.Intrinsic.Control.EndIf
	F.Intrinsic.Control.EndIf

	F.ODBC.conx!rstTemplate.Close

	Gui.F_Template..Enabled(true)
F.Intrinsic.Control.EndIf

program.sub.drpdisplay_click.end

